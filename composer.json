{
    "name"        : "jbzoo/ssmaker-server",
    "description" : "Alternative server for SSmaker",
    "keywords"    : ["ssmaker", "server", "client", "jbzoo", "screenshort"],
    "homepage"    : "https://github.com/JBZoo/SSmaker-Server",
    "license"     : "MIT",
    "authors"     : [
        {
            "name"  : "SmetDenis",
            "email" : "denis@jbzoo.com",
            "role"  : "lead"
        }
    ],
    "require"     : {
        "php"               : ">=5.5.0",
        "ext-soap"          : "*",
        "slim/slim"         : "^3.1",
        "slim/php-view"     : "^2.0",
        "slim/twig-view"    : "^2.1",
        "jbzoo/image"       : "^3.0",
        "jbzoo/utils"       : "^1.7",
        "jbzoo/less"        : "^1.0",
        "oyejorge/less.php" : "v1.7.0.10"
    },
    "require-dev" : {
        "jbzoo/phpunit"       : "^1.9",
        "jbzoo/jbdump"        : "^1.5",
        "symfony/var-dumper"  : "^3.1",
        "jbzoo/http-client"   : "^1.1",
        "ulrichsg/getopt-php" : "^2.3"
    },
    "extra"       : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    },
    "config"      : {
        "optimize-autoloader" : true
    },
    "autoload"    : {
        "psr-4" : {
            "JBZoo\\SSmakerServer\\" : "src"
        }
    }
}
